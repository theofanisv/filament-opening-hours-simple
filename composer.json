{
    "name": "theofanisv/filament-opening-hours-simple",
    "description": "Comprehensive Filament v4 UI components for spatie/opening-hours",
    "keywords": [
        "filament",
        "opening-hours",
        "laravel",
        "spatie",
        "filament-plugin",
        "business-hours"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Theofanis Vathis",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "filament/filament": "^4.0",
        "spatie/opening-hours": "^4.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/phpstan": "^2.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Theofanisv\\FilamentOpeningHours\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Theofanisv\\FilamentOpeningHours\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Theofanisv\\FilamentOpeningHours\\FilamentOpeningHoursServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    }
}
